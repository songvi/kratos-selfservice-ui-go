{
    "kratos": {
        "services": {
            "admin": "http://127.0.0.1:1234",
            "public": "http://127.0.0.1:1235"
        },
        "browser": {
            "init-path": "/self-service/browser/flows",
            "request-path": "/self-service/browser/flows/requests"
        },
        "api": {}
    },
    "listen": "127.0.0.1:1991",
    "templatedir": "templates",
    "endpoints": {
        "profile": "/profile",
        "login": "/auth/login",
        "verification": "/verify",
        "error": "/error",
        "multifactors": "/mfa",
        "registration": "/auth/registration",
        "logout": "/logout"
    },
    "oauth2": {
        "login_provider": "/oauth/login",
        "consent_provider": "/oauth/consent",
        "login_api_url": "/oauth2/auth/requests/login",
        "consent_api_url": "/oauth2/auth/requests/consent"
    }
}